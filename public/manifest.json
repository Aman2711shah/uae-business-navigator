{
  "id": "/",
  "name": "UAE Business Navigator",
  "short_name": "UAEBiz",
  "description": "Complete UAE Business Setup & Services Platform",
  "lang": "en-US",
  "start_url": "/?utm_source=homescreen",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "tabbed", "standalone", "minimal-ui"],
  "background_color": "#ffffff",
  "theme_color": "#0ea5a4",
  "orientation": "portrait-primary",
  "scope": "/",
  "dir": "ltr",
  "iarc_rating_id": "PENDING_IARC",
  "prefer_related_applications": true,
  "related_applications": [
    {
      "platform": "play",
      "id": "com.aman.uaebiznav",
      "url": "https://play.google.com/store/apps/details?id=com.aman.uaebiznav"
    }
  ],
  "scope_extensions": [
    {
      "origin": "https://wazeet.lovable.app"
    }
  ],
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "shortcuts": [
    {
      "name": "New Application",
      "short_name": "Apply",
      "description": "Start a new business application",
      "url": "/business-setup",
      "icons": [
        {
          "src": "/icons/icon-192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Track Application",
      "short_name": "Track",
      "description": "Track your existing applications",
      "url": "/track-application",
      "icons": [
        {
          "src": "/icons/icon-192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    }
  ],
  "file_handlers": [
    {
      "action": "/my-documents",
      "accept": {
        "application/pdf": [".pdf"],
        "image/*": [".jpg", ".jpeg", ".png", ".gif", ".webp"],
        "text/plain": [".txt"],
        "text/csv": [".csv"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+uaebiz",
      "url": "/?protocol=%s"
    }
  ],
  "share_target": {
    "action": "/my-documents",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "documents",
          "accept": ["application/pdf", "image/*", "text/plain"]
        }
      ]
    }
  },
  "icons": [
    {
      "src": "/icons/icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/icons/icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/screen-wide-1.png",
      "sizes": "1280x720",
      "type": "image/png",
      "label": "Home - wide"
    },
    {
      "src": "/screenshots/screen-wide-2.png",
      "sizes": "1280x720",
      "type": "image/png",
      "label": "Services - wide"
    },
    {
      "src": "/screenshots/screen-narrow-1.png",
      "sizes": "720x1280",
      "type": "image/png",
      "label": "Dashboard - narrow"
    },
    {
      "src": "/screenshots/screen-narrow-2.png",
      "sizes": "720x1280",
      "type": "image/png",
      "label": "Calculator - narrow"
    }
  ],
  "categories": ["business", "productivity"]
}